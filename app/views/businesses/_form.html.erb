<%= simple_form_for [@user, @business] do |f| %>
  <%= f.error_notification %>
  <div class="content-div">
    <div class="form-inputs">
      <%= f.input :business_name %>
      <%= f.input :business_start %>
      <%= f.input :business_street_1 %>
      <%= f.input :business_street_2 %>
      <%= f.input :business_city %>
      <%= f.input :business_state %>
      <%= f.input :business_zip %>
      <%= f.input :ein %>
      <%= f.input :biz_type %>
      <%= f.input :bac %>
      <%= f.input :method %>
      <%= f.input :activity %>
      <%= f.input :product %>
      <%= f.input :schb3 %>
      <%= f.input :schb4a %>
      <%= f.input :schb4b %>
      <%= f.input :schb10a %>
      <%= f.input :schb10b %>
    </div>
  
    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>
